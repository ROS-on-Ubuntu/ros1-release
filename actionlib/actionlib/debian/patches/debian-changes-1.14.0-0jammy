+  ros::NodeHandle n;
+
+  actionlib::ServiceServer service = actionlib::advertiseService<actionlib::TwoIntsAction>(n,
+      "add_two_ints",
+      boost::bind(add, _1, _2));
+
+  ros::spin();
+
+  return 0;
