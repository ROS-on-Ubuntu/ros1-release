+        <geometry>
+          <mesh filename="package://pr2_description/meshes/shoulder_v0/shoulder_pan.stl"  />
+        </geometry>
+      </collision>
+    </link>
+
+    <!-- Shoulder lift -->
+    <joint name="${side}_shoulder_lift_joint" type="revolute">
+      <axis xyz="0 1 0"/>
