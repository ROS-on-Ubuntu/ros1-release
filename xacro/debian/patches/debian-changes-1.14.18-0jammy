+    filename = abs_filename_spec(filename)
+    f = open(filename)
+    filestack.append(filename)
+    try:
+        return YamlListWrapper.wrap(yaml.safe_load(f))
+    finally:
+        f.close()
+        filestack.pop()
+        global all_includes
+        all_includes.append(filename)
