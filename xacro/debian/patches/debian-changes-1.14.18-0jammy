+def load_yaml(filename):
+    try:
+        import yaml
+        for unit in ConstructUnits:
+            yaml.SafeLoader.add_constructor(unit.value.tag, unit.constructor)
+    except Exception:
+        raise XacroException("yaml support not available; install python-yaml")
