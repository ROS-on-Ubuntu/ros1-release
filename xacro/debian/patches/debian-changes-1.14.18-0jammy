+def grab_property(elt, table):
+    assert(elt.tagName == 'xacro:property')
+    remove_previous_comments(elt)
+
+    name, value, default, remove, scope, lazy_eval = \
+        check_attrs(elt, ['name'], ['value', 'default', 'remove', 'scope', 'lazy_eval'])
