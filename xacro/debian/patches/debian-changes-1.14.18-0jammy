+      <origin xyz="0 0 0" rpy="0 0 0" />
+    </xacro:pr2_upper_arm_roll_v0>
+
+    <!-- extensions -->
+    <xacro:pr2_shoulder_gazebo_v0 side="${side}" />
+    <xacro:pr2_shoulder_transmission_v0 side="${side}" />
