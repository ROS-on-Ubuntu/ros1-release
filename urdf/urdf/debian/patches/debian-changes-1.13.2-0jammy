+#ifndef URDF__MODEL_H_
+#define URDF__MODEL_H_
+
+#include <string>
+
+#include <urdf_model/model.h>
+
+#include <urdf/urdfdom_compatibility.h>
+
+#include <tinyxml.h>
+
